<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- The INTERNET permission is required for development. Specifically,
         the Flutter tool needs it to communicate with the running application
         to allow setting breakpoints, to provide hot reload, etc.
    -->
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <!-- AAOS DISTRACTION OPTIMIZATION - PROFILE BUILD -->
    <uses-permission android:name="android.car.permission.CAR_DRIVING_STATE" />
    <uses-permission android:name="android.car.permission.CAR_UX_RESTRICTIONS_CONFIGURATION" />
    
    <application>
        <!-- PROFILE BUILD: MARK AS DISTRACTION OPTIMIZED -->
        <meta-data
            android:name="com.android.automotive.application.DISTRACTION_OPTIMIZED"
            android:value="true" />
    </application>
</manifest>
